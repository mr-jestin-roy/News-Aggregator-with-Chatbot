# .env_example

# URL your React/Vite app will use to talk to the Flask API
VITE_API_URL=http://localhost:5000

# Your OpenAI API key (get one from https://platform.openai.com/account/api-keys)
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Flask debug mode (set to "True" or "False")
FLASK_DEBUG=True

# Port your Flask app will listen on
PORT=8000
